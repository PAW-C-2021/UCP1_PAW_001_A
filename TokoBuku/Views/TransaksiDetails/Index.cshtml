@model IEnumerable<TokoBuku.Models.TransaksiDetail>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KodeBukuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KodeTransaksiNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KodeBukuNavigation.KodeBuku)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KodeTransaksiNavigation.KodeTransaksi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTransaksiDetail">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTransaksiDetail">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTransaksiDetail">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
